source 'https://github.com/CocoaPods/Specs.git'
source 'https://github.com/shvets/Specs.git'

use_frameworks!

target 'Example' do
  platform :ios, '10.0'

  pod 'AudioPlayer', :path => '../'
  pod 'WebAPI', :path => '../../WebAPI'
  
  podspec :path => 'Example.podspec'

  target 'ExampleTests' do
    inherit! :search_paths
  end
end

